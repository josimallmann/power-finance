<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.PowerFinance" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">#00796B</item>
        <item name="colorPrimaryVariant">#FF000000</item>
        <item name="colorOnPrimary">#FF000000</item>
        <item name="colorSecondary">#FF000000</item>
        <item name="colorSecondaryVariant">#FF000000</item>
        <item name="colorOnSecondary">#FF000000</item>
        <item name="android:statusBarColor">#FF000000</item>
        <item name="android:windowBackground">#FF000000</item>
        <item name="android:actionBarStyle">@style/PowerFinanceActionBar</item>
